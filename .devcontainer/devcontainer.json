{
    "name": "Project Selection",
    "hostRequirements": {
        "cpus": 2,
        "memory": "4gb",
        "storage": "32gb"
    },
    "updateContentCommand": "echo 'Select a project to continue setup'",
    "customizations": {
        "vscode": {
            "settings": {},
            "extensions": []
        },
        "codespaces": {
            "openFiles": ["README.md"]
        }
    },
    "remoteUser": "root",
    "features": {
        "ghcr.io/devcontainers/features/common-utils:2": {}
    },
    "remoteEnv": {},
    "forwardPorts": [],
    "_devcontainer": {
        "configFiles": [
            {
                "path": "project-A/.devcontainer/devcontainer.json",
                "label": "Project A"
            },
            {
                "path": "project-B/.devcontainer/devcontainer.json",
                "label": "Project B"
            }
        ]
    }
}
